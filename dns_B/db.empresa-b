$TTL 604800
@   IN  SOA empresa-b.com. root.empresa-b.com. (
            3        ; Serial (incremente este número a cada alteração)
            604800   ; Refresh
            86400    ; Retry
            2419200  ; Expire
            604800 ) ; Minimum TTL
;
@       IN  NS      ns1.empresa-b.com.
@       IN  A       192.168.1.100     ; Endereço IP do domínio principal
ns1     IN  A       192.168.1.1       ; Endereço IP do servidor DNS
